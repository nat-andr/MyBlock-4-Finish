import tkinter

master=tkinter.Tk()
master.title("grid() method")
master.geometry("650x650")

button1=tkinter.Button(master, text="button1  row=1,column=0")
button1.grid(row=1,column=0)

button2=tkinter.Button(master, text="button2 row=2,column=2")
button2.grid(row=2,column=2)

button3=tkinter.Button(master, text="button3  row=3,column=3")
button3.grid(row=3,column=3)

button4=tkinter.Button(master, text="button4 row=4,column=4")
button4.grid(row=4,column=4)

button5=tkinter.Button(master, text="button5  row=5,column=5")
button5.grid(row=5,column=5)

button6=tkinter.Button(master, text="button6  row=6,column=0")
button6.grid(row=6,column=0)

button7=tkinter.Button(master, text="button7 row=7,column=2")
button7.grid(row=7,column=2)

button8=tkinter.Button(master, text="button8  row=8,column=3")
button8.grid(row=8,column=3)

button9=tkinter.Button(master, text="button9 row=9,column=4")
button9.grid(row=9,column=4)

button10=tkinter.Button(master, text="button10  row=10,column=5")
button10.grid(row=10,column=5)

button11=tkinter.Button(master, text="button11  row=11,column=0")
button11.grid(row=11,column=0)

button12=tkinter.Button(master, text="button12 row=12,column=2")
button12.grid(row=12,column=2)

button13=tkinter.Button(master, text="button13  row=13,column=3")
button13.grid(row=13,column=3)

button14=tkinter.Button(master, text="button14 row=14,column=4")
button14.grid(row=14,column=4)

button15=tkinter.Button(master, text="button15  row=15,column=5")
button15.grid(row=15,column=5)

button16=tkinter.Button(master, text="button16  row=16,column=0")
button16.grid(row=16,column=0)

button17=tkinter.Button(master, text="button17 row=17,column=2")
